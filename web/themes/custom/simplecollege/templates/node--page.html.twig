<article{{ attributes }}>

  {{ title_prefix }}
  {% if label and not page %}

    <h2{{ title_attributes }}>
      <a href="{{ url }}" rel="bookmark">{{ label }}</a>
    </h2>

  {% endif %}
  {{ title_suffix }}

  {% if display_submitted %}
    <footer>
      {{ author_picture }}
      <div{{ author_attributes }}>
        {% trans %}Submitted by {{ author_name }} on {{ date }}{% endtrans %}
      </div>
    </footer>
  {% endif %}

  <div{{ content_attributes }}>
    {{ content }}
  </div>
{% if view_mode == 'full' %}
    {% trans %} <h3> Please report the issues on this page to developer... <h3> {% endtrans %}
{% endif %}
</article>
